    "games",
    "discord-games",
    "djs-games"
  ],
  "author": "HiZollo Organization",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HiZollo/Games/issues"
  },
  "homepage": "https://github.com/HiZollo/Games#readme",
  "devDependencies": {
    "discord.js": "^13.8.0",
    "typescript": "^4.7.3"
  }
}
